          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH last AS (\r\n  SELECT max(run_ts) AS run_ts\r\n  FROM mart.ur_link_scores\r\n)\r\nSELECT\r\n  dc.name AS name_end,\r\n  round(avg(s.p_demand)::numeric, 3) AS avg_p_in,\r\n  round(sum(s.p_demand)::numeric, 3) AS total_p_in,\r\n  count(*) FILTER (WHERE s.p_demand > 0.7) AS strong_links\r\nFROM mart.ur_link_scores s\r\nJOIN last t ON s.run_ts = t.run_ts\r\nJOIN raw.dist_cent dc\r\n  ON dc.no = s.no_end\r\nGROUP BY dc.name\r\nORDER BY total_p_in DESC\r\nLIMIT 30;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
