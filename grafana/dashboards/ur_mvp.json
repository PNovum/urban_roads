        {
          "dataset": "urmvp",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "(\r\n  with p as (\r\n    select\r\n      no_start, no_end, name_start, name_end,\r\n      trip_cnt_pred::int as pred\r\n    from mart.ur_trip_preds_latest\r\n  ),\r\n  a as (\r\n    select\r\n      no_start, no_end,\r\n      trip_cnt as actual_cnt,\r\n      case when trip_cnt >= 1 then 1 else 0 end as actual_bin\r\n    from mart.taxi_agg\r\n  )\r\n  select\r\n    'FN' as err_type,\r\n    p.name_start,\r\n    p.name_end,\r\n    p.pred as pred_bin,\r\n    coalesce(a.actual_cnt, 0) as actual_trip_cnt\r\n  from p\r\n  join a\r\n    on a.no_start = p.no_start and a.no_end = p.no_end\r\n  where p.pred = 0 and a.actual_bin = 1\r\n  order by a.actual_cnt desc\r\n  limit 20\r\n)\r\nunion all\r\n(\r\n  with p as (\r\n    select\r\n      no_start, no_end, name_start, name_end,\r\n      trip_cnt_pred::int as pred\r\n    from mart.ur_trip_preds_latest\r\n  ),\r\n  a as (\r\n    select\r\n      no_start, no_end,\r\n      trip_cnt as actual_cnt,\r\n      case when trip_cnt >= 1 then 1 else 0 end as actual_bin\r\n    from mart.taxi_agg\r\n  )\r\n  select\r\n    'FP' as err_type,\r\n    p.name_start,\r\n    p.name_end,\r\n    p.pred as pred_bin,\r\n    coalesce(a.actual_cnt, 0) as actual_trip_cnt\r\n  from p\r\n  left join a\r\n    on a.no_start = p.no_start and a.no_end = p.no_end\r\n  where p.pred = 1 and coalesce(a.actual_bin, 0) = 0\r\n  order by coalesce(a.actual_cnt, 0) asc, p.name_start, p.name_end\r\n  limit 20\r\n);\r\n",
          "refId": "A",
